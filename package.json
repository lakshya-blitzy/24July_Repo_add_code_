{
  "name": "node-http-tutorial",
  "version": "1.0.0",
  "description": "Node.js HTTP server tutorial project for Blitzy platform integration testing",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest integration.test.js",
    "lint": "eslint *.js",
    "lint:fix": "eslint *.js --fix",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:logs": "pm2 logs"
  },
  "keywords": [
    "nodejs",
    "http",
    "express",
    "tutorial",
    "api",
    "server",
    "blitzy",
    "backprop"
  ],
  "author": "Blitzy Platform",
  "license": "MIT",
  "dependencies": {
    "express": "^5.1.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^8.0.1",
    "dotenv": "^17.2.0",
    "winston": "^3.17.0",
    "compression": "^1.7.4",
    "body-parser": "^1.20.2",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "supertest": "^6.3.0",
    "nodemon": "^3.0.0",
    "eslint": "^8.57.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blitzy/node-http-tutorial"
  },
  "bugs": {
    "url": "https://github.com/blitzy/node-http-tutorial/issues"
  },
  "homepage": "https://github.com/blitzy/node-http-tutorial#readme"
}